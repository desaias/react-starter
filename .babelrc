{
    "presets": [
        ["es2015", { "modules": false }],
        "react",
        "stage-0"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-flow-strip-types"
    ],
    "env": {
        "dev": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        },
        "test": {
            "plugins": [
                [
                    "__coverage__",
                    { "ignore": "*.text.*" }
                ]
            ]
        }
    }
}
